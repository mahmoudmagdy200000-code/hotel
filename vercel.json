{
    "version": 2,
    "builds": [
        {
            "src": "front/hotel-frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/front/hotel-frontend/$1"
        }
    ]
}